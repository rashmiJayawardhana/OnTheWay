{
  "expo": {
    "name": "OnTheWay",
    "slug": "OnTheWay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/flight-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/flight-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/flight-icon.png"
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2f80ed",
      "imageWidth": 200
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://ontheway.com/"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}